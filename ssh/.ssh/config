Host aur.archlinux.org aur4.archlinux.org
    IdentityFile ~/.ssh/id_rsa
    User aur

Host fenrir sdl.ist.osaka-u.ac.jp
    IdentityFile ~/.ssh/id_rsa
    User jc-yang
    ControlMaster auto
    ControlPath ~/.ssh/socket-fenrir-%C
    ControlPersist yes
    ServerAliveInterval 10

